t = 5
split_cols = split(c(1:ncol(m)), ceiling(seq_along(1:ncol(m))/100))
m_sub <- lapply(split_cols, function(x){
sample_m <- m[,x]
sample_m <- sample_m[,apply(sample_m, 2, function(x) length(which(x != 0))) > t]
})
